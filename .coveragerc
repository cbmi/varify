[run]
branch = True
source = varify

[report]
omit =
    varify/admin.py
    varify/migrations/*
    varify/*/migrations/*
    varify/__init__.py
    varify/*/__init__.py
    varify/*/*/__init__.py
    varify/*/*/*/__init__.py

exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    def __unicode__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplemented(Error)?
    if 0:
    if __name__ == .__main__.:

[html]
directory = docs/coverage
